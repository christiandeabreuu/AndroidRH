<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navegacao"
    app:startDestination="@id/cadastroFragment">

    <fragment
        android:id="@+id/cadastroFragment"
        android:name="br.com.zup.rhcatalisa.CadastroFragment"
        android:label="fragment_cadastro"
        tools:layout="@layout/fragment_cadastro" >
        <action
            android:id="@+id/action_cadastroFragment_to_mostrarFragment"
            app:destination="@id/mostrarFragment" />
    </fragment>
    <fragment
        android:id="@+id/mostrarFragment"
        android:name="br.com.zup.rhcatalisa.MostrarFragment"
        android:label="fragment_mostrar"
        tools:layout="@layout/fragment_mostrar" />
</navigation>